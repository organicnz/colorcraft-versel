{
  "name": "colorcraft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "vercel-build": "node scripts/verify-env.js || true && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next",
    "analyze": "ANALYZE=true next build",
    "logs": "vercel logs",
    "logs:latest": "vercel logs $(vercel ls -l 1 --output tsv | tail -1 | cut -f1)",
    "logs:json": "vercel logs $(vercel ls -l 1 --output tsv | tail -1 | cut -f1) --json",
    "logs:analyze": "node scripts/analyze-vercel-logs.js",
    "logs:monitor": "bash scripts/vercel_log_checker.sh",
    "logs:monitor:background": "bash scripts/vercel_log_checker.sh 600 >/dev/null 2>&1 &",
    "logs:setup-cron": "bash scripts/cron_vercel_logs.sh",
    "logs:build": "node scripts/fetch-deployment-logs.js build",
    "logs:runtime": "node scripts/fetch-deployment-logs.js runtime",
    "logs:trigger": "node scripts/trigger-deployment-activity.js",
    "lint:fix": "bash scripts/fix-linter-errors.sh",
    "setup:husky": "bash scripts/setup-husky.sh",
    "prepare": "husky",
    "ls": "vercel ls",
    "deploy": "bash ./scripts/deploy.sh",
    "deploy:prod": "vercel --prod",
    "deploy:robust": "bash scripts/vercel-deploy.sh",
    "deploy:robust:prod": "bash scripts/vercel-deploy.sh production",
    "webhook": "node scripts/vercel-webhook.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/lib/db/migrate-with-env.ts",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "check-env": "node scripts/check-env.js",
    "test-db": "node scripts/test-connection.js",
    "verify-db": "node scripts/verify-db-connection.js",
    "fix-routes": "node scripts/fix-route-conflicts.js",
    "verify-env": "node scripts/verify-env.js",
    "test:email": "node scripts/test-email.js",
    "test:email:reset": "node scripts/test-custom-emails.js reset",
    "test:email:verify": "node scripts/test-custom-emails.js verify",
    "diagnose": "node scripts/verify-env.js && node scripts/verify-db-connection.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-query": "^5.69.0",
    "@uploadthing/react": "^7.3.0",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.41.0",
    "framer-motion": "^11.0.5",
    "lucide-react": "^0.483.0",
    "next": "^14.2.25",
    "next-themes": "^0.4.6",
    "pg": "^8.11.3",
    "postcss": "^8.4.24",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-datepicker": "^8.2.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "resend": "^4.1.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3",
    "tw-animate-css": "^1.2.4",
    "uploadthing": "^7.5.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3",
    "typescript": "^5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^14.1.0",
    "@tanstack/react-query-devtools": "^5.71.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/pg": "^8.11.3",
    "@types/react": "^18",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.30.6",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsx": "^4.19.3"
  },
  "engines": {
    "node": ">=18.x"
  }
}
