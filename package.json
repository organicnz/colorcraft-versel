{
  "name": "colorcraft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:legacy": "next dev",
    "build": "next build",
    "vercel-build": "node scripts/verify-env.js || true && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next",
    "analyze": "ANALYZE=true next build",
    "logs": "vercel logs",
    "logs:latest": "vercel logs $(vercel ls -l 1 --output tsv | tail -1 | cut -f1)",
    "logs:json": "vercel logs $(vercel ls -l 1 --output tsv | tail -1 | cut -f1) --json",
    "logs:analyze": "node scripts/analyze-vercel-logs.js",
    "logs:monitor": "bash scripts/vercel_log_checker.sh",
    "logs:monitor:background": "bash scripts/vercel_log_checker.sh 600 >/dev/null 2>&1 &",
    "logs:setup-cron": "bash scripts/cron_vercel_logs.sh",
    "logs:build": "node scripts/fetch-deployment-logs.js build",
    "logs:runtime": "node scripts/fetch-deployment-logs.js runtime",
    "logs:trigger": "node scripts/trigger-deployment-activity.js",
    "lint:fix": "bash scripts/fix-linter-errors.sh",
    "setup:husky": "bash scripts/setup-husky.sh",
    "prepare": "husky",
    "ls": "vercel ls",
    "deploy": "bash ./scripts/deploy.sh",
    "deploy:prod": "vercel --prod",
    "deploy:robust": "bash scripts/vercel-deploy.sh",
    "deploy:robust:prod": "bash scripts/vercel-deploy.sh production",
    "webhook": "node scripts/vercel-webhook.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/lib/db/migrate-with-env.ts",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "check-env": "node scripts/check-env.js",
    "test-db": "node scripts/test-connection.js",
    "verify-db": "node scripts/verify-db-connection.js",
    "fix-routes": "node scripts/fix-route-conflicts.js",
    "verify-env": "node scripts/verify-env.js",
    "test:email": "node scripts/test-email.js",
    "test:email:reset": "node scripts/test-custom-emails.js reset",
    "test:email:verify": "node scripts/test-custom-emails.js verify",
    "diagnose": "node scripts/verify-env.js && node scripts/verify-db-connection.js",
    "commit": "node scripts/auto-commit.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@tanstack/react-query": "^5.80.6",
    "@uploadthing/react": "^7.3.1",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "framer-motion": "^12.16.0",
    "lucide-react": "^0.513.0",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "pg": "^8.16.0",
    "postcss": "^8.5.4",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "resend": "^4.5.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "uploadthing": "^7.7.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.3.3",
    "@tanstack/react-query-devtools": "^5.80.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "30.0.0-beta.3",
    "msw": "^2.10.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.19.4"
  },
  "engines": {
    "node": ">=22.x"
  }
}
