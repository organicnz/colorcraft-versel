{
  "name": "colorcraft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:legacy": "next dev",
    "build": "next build",
    "prebuild": "echo 'Starting prebuild...' && node scripts/verify-env.js || true",
    "build:production": "echo 'Building for production...' && next build",
    "vercel-build": "node scripts/verify-env.js || true && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:bundle": "node scripts/bundle-analyzer.js",
    "logs": "vercel logs",
    "logs:latest": "vercel logs $(vercel ls -l 1 --output tsv | tail -1 | cut -f1)",
    "logs:debug": "vercel logs $(vercel ls -l 1 --output tsv | tail -1 | cut -f1) | head -200",
    "fetch:logs": "node scripts/fetch-deployment-logs.js",
    "deploy": "vercel",
    "prod": "vercel --prod",
    "staging": "vercel --no-prod",
    "env:pull": "vercel env pull .env.local",
    "env:push": "vercel env add",
    "check:env": "node scripts/check-env.js",
    "setup:env": "node scripts/setup-env.js",
    "logs:cleanup": "./scripts/cleanup-logs.sh",
    "logs:monitor": "./scripts/vercel_log_checker.sh",
    "logs:analyze": "node scripts/analyze-vercel-logs.js",
    "git:fix": "node scripts/fix-route-conflicts.js",
    "webhook": "node scripts/vercel-webhook.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check-env": "node scripts/check-env.js",
    "test-db": "node scripts/test-connection.js",
    "setup-db": "node scripts/setup-supabase.js",
    "test:db": "node scripts/test-connection.js",
    "test:env": "node scripts/test-env.js",
    "test:auth": "node scripts/test-auth-flow.js",
    "test:api": "node scripts/test-api-routes.js",
    "debug:storage": "node scripts/debug-portfolio-storage.js",
    "test:email": "node scripts/test-email.js",
    "test:email:portfolio": "node scripts/test-custom-emails.js portfolio",
    "test:email:contact": "node scripts/test-custom-emails.js contact",
    "test:email:verify": "node scripts/test-custom-emails.js verify",
    "diagnose": "node scripts/verify-env.js && node scripts/verify-db-connection.js",
    "commit": "./scripts/commit.sh",
    "optimize": "node scripts/optimize-codebase.js",
    "perf:monitor": "node scripts/performance-monitor.js",
    "perf:bundle": "npm run analyze:bundle",
    "perf:lighthouse": "npx lighthouse http://localhost:3000 --output json --output html --output-path ./lighthouse-report",
    "clean:all": "rm -rf .next node_modules/.cache .vercel && npm install",
    "clean:install": "npm run clean && npm install",
    "health": "node -e \"console.log('âœ… Node.js health check passed')\"",
    "env:check": "node scripts/verify-env.js",
    "deps:check": "npx depcheck",
    "deps:update": "npx npm-check-updates -u && npm install",
    "security:audit": "npm audit --audit-level=high",
    "build:analyze": "cross-env ANALYZE=true npm run build && npm run analyze:bundle",
    "dev:performance": "cross-env NODE_ENV=development npm run dev & npm run perf:monitor",
    "production:test": "npm run build && npm run start"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@tanstack/react-query": "^5.80.6",
    "@uploadthing/react": "^7.3.1",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "framer-motion": "^12.16.0",
    "lucide-react": "^0.513.0",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "pg": "^8.16.0",
    "postcss": "^8.5.4",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "resend": "^4.5.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "uploadthing": "^7.7.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.3.3",
    "@tanstack/react-query-devtools": "^5.80.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.4.1",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "30.0.0-beta.3",
    "msw": "^2.10.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.19.4",
    "web-vitals": "^5.0.2"
  },
  "engines": {
    "node": ">=22.x"
  }
}
